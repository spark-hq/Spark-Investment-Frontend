# Environment Configuration for Spark Investment Frontend

# Mock mode (development without backend)
# Set to 'true' to use mock data, 'false' to connect to real API
VITE_MOCK_MODE=true

# API Configuration
# Base URL for backend API (used when MOCK_MODE is false)
VITE_API_BASE_URL=http://localhost:5000/api

# Debug mode
# Set to 'true' to enable console logging and debug messages
VITE_DEBUG_MODE=false
